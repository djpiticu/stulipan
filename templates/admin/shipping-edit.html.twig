{% extends 'admin/template-editing.html.twig' %}

{% set pageTitle = title %}
{% set backToText = 'Szállítási módok' %}
{% set breadcrumb = {
	0: {
		'url': path('settings-home'),
		'text': 'settings.settings'|trans,
	},
	1: {
		'url': path('shipping-list'),
		'text': 'Szállítási módok',
	}
} %}
{% set cancelUrl = path('shipping-list') %}

{% block title %}
	{{ pageTitle }}
{% endblock %}

{% block moduleTitle %}
	Szállítási mód
{% endblock %}

{% block moduleDescription %}
	Vegyél fel új szállítási módot.
{% endblock %}


{% block contentInner %}
	{% include '/admin/_shipping-form.html.twig' with {'cancelUrl': cancelUrl} %}
{% endblock %}